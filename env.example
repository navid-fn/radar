# ClickHouse Configuration
CLICKHOUSE_DB=default
CLICKHOUSE_USER=default
CLICKHOUSE_PASSWORD=password
CLICKHOUSE_HTTP_PORT=8123
CLICKHOUSE_TCP_PORT=9000

# Kafka Configuration
KAFKA_BROKER_ID=1
KAFKA_LISTENERS=PLAINTEXT://localhost:9092
